.row-margin-xs {
  margin-top: 0.8rem; }

.row-margin-sm {
  margin-top: 1.3rem; }

.row-margin-md {
  margin-top: 2.1rem; }

.row-margin-lg {
  margin-top: 3.4rem; }

.row-margin-xl {
  margin-top: 5.5rem; }

html,
body {
  width: 100%;
  margin: 0;
  padding: 0;
  font-family: 'canada-type-gibson';
  font-size: 10px;
  background-color: #f0f0f0; }

#app {
  display: flex;
  flex-direction: column;
  width: 100%;
  min-width: 98.7rem;
  min-height: calc(100Vh - 0.8rem); }

#page {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  flex: 1;
  margin-bottom: 4.8rem; }



#app-footer {
  width: 100%;
  height: 0.8rem;
  background-color: #dd0426; }

#nav {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  min-width: 136.6rem;
  height: 8rem;
  background-color: #fff;
  border-bottom: 1px solid #e5e5e5;
  z-index: 999999; }

#nav_elements {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 8rem; }

#nav_logo-container {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  width: 23.3rem; }

#nav_logo {
  padding-left: 1.6rem;
  height: 3.2rem; }

#nav_menu {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  flex: 1;
  height: 100%;
  font-size: 1.4rem;
  line-height: 1.4rem; }

.nav_menu-item {
  position: relative;
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 100%; }

.nav_menu-active-indicator {
  position: absolute;
  bottom: 0rem;
  width: 100%;
  height: 0.4rem;
  background-color: #dd0426; }

.nav_menu-spacer {
  margin-left: 1.6rem;
  margin-right: 1.6rem;
  color: #b3b3b3; }

a.nav_link:link {
  font-weight: 600;
  color: #1A1A1A;
  text-decoration: none; }

a.nav_link:hover {
  color: #dd0426; }

a.nav_link:visited {
  color: #1A1A1A; }

a.nav_link:active {
  color: #dd0426; }

.nav_active {
  color: #dd0426; }

#nav_user {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-end;
  width: 23.3rem;
  padding-right: 1.6rem;
  box-sizing: border-box; }

#nav_user-icon {
  width: 3.2rem;
  height: 3.2rem;
  border-radius: 50%;
  background-size: cover;
  background-position: center; }

.nav_menu-item-right {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  margin-left: 1.6rem;
  font-size: 2rem;
  line-height: 2rem; }

a.nav_settings:link {
  color: #b3b3b3;
  text-decoration: none; }

a.nav_settings:hover {
  color: #dd0426; }

a.nav_settings:visited {
  color: #b3b3b3; }

a.nav_settings:active {
  color: #b3b3b3; }

#subnav {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 100rem;
  margin-top: 11.4rem;
  padding-right: 1.6rem;
  padding-left: 1.6rem;
  box-sizing: border-box;
  font-size: 1.4rem;
  line-height: 1.4rem;
  color: #b3b3b3; }

#subnav_title {
  font-size: 1.4rem;
  line-height: 1.4rem;
  text-align: left; }

.subnav_title-bold {
  padding-left: 0.5rem;
  padding-right: 0.5rem;
  box-sizing: border-box;
  font-weight: 600;
  color: #808080; }

.subnav_active {
  padding-left: 0.8rem;
  box-sizing: border-box;
  color: #3d9720;
  font-weight: 600; }

.subnav_right-side {
  display: flex;
  align-items: center;
  justify-content: flex-end;
  flex: 1; }
  .subnav_right-side span {
    padding-right: 0.4rem;
    color: #dd0426; }



a.subnav_deactivate:link {
  font-size: 1.4rem;
  line-height: 1.4rem;
  font-weight: 600;
  color: #f0a202;
  text-decoration: none; }

a.subnav_deactivate:hover {
  color: #1A1A1A; }

a.nav_settings:visited {
  color: #f0a202; }

a.nav_settings:active {
  color: #f0a202; }

.subnav_menu-spacer {
  margin-left: 1.6rem;
  margin-right: 1.6rem;
  color: #b3b3b3; }

.form_input {
  display: block;
  height: 3.4rem;
  width: 61.1rem;
  padding-left: 2.1rem;
  box-shadow: none;
  -webkit-appearance: none;
  font-size: 1.3rem;
  line-height: 1.3rem;
  font-weight: 400; }

.form_multi {
  padding-top: 1.2rem;
  box-sizing: border-box;
  height: 13.2rem; }

.form_select {
  width: 61.1rem;
  height: 3.4rem;
  padding-left: 2.1rem;
  box-shadow: none;
  font-size: 1.3rem;
  font-weight: 400; }

.form_required {
  font-size: 1.3rem;
  font-weight: 300;
  font-style: italic;
  color: #3f88c5; }

.form_empty {
  color: #b3b3b3; }

.form_filled {
  color: #1A1A1A; }



.error {
  box-sizing: border-box;
  background: transparent;
  border-top: 1px solid #dd0426;
  border-bottom: 1px solid #dd0426;
  border-left: 1px solid #dd0426;
  border-right: 1px solid #dd0426;
  border-radius: 0.1rem 0.1rem 0.1rem 0.1rem;
  outline: 0;
  color: #dd0426;
  background-color: #f8dee0; }

.form_error-header {
  width: 100%;
  padding: 1.3rem;
  border-top: 1px solid #dd0426;
  border-bottom: 1px solid #dd0426;
  border-left: 1px solid #dd0426;
  border-right: 1px solid #dd0426;
  border-radius: 0.1rem 0.1rem 0.1rem 0.1rem;
  font-size: 1.1rem;
  font-weight: 300;
  color: #dd0426;
  background-color: #f8dee0; }

.form_error-header-title {
  font-weight: 400; }






.dash_container {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: center;
  width: 100rem;
  margin-top: 11.4rem;
  padding: 1.6rem 1.6rem 3.2rem 1.6rem;
  box-sizing: border-box;
  background-color: #fff;
  border: 1px solid #e5e5e5;
  border-radius: 0.3rem 0.3rem 0.3rem 0.3rem;
  box-shadow: 0px 1px 1px #e5e5e5; }

.dash_chart-container {
  width: 100%;
  margin-bottom: 4.8rem;
  padding-bottom: 1.6rem;
  border-bottom: 1px solid #e5e5e5; }

.dash_chart-title {
  display: flex;
  flex-direction: row;
  justify-content: center;
  font-size: 1.4rem;
  line-height: 1.4rem;
  color: #1A1A1A; }

.dash_widget-left {
  width: calc(50% - 1.6rem);
  margin-right: 1.6rem; }

.dash_widget-right {
  width: calc(50% - 1.6rem);
  margin-left: 1.6rem; }

.dash_widget-heading {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  font-size: 2rem;
  line-height: 2rem;
  font-weight: 600;
  text-transform: uppercase; }

.dash_widget-content {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 100%; }

.dash_widget-row {
  display: flex;
  flex-direction: row;
  width: 100%;
  margin-top: 1.6rem;
  font-size: 1.4rem;
  line-height: 1.4rem; }

.dash_widget-label {
  width: 50%;
  padding-right: 1.6rem;
  box-sizing: border-box;
  text-align: right;
  color: #b3b3b3; }

a.dash_link:link {
  font-size: 1.4rem;
  line-height: 1.4rem;
  font-weight: 600;
  color: #dd0426;
  text-decoration: none; }

a.dash_link:hover {
  color: #1A1A1A; }

a.dash_link:visited {
  color: #dd0426; }

a.dash_link:active {
  color: #dd0426; }

.dash_widget-emphasis {
  font-weight: 600;
  color: #1A1A1A; }

.dash_label-normal {
  color: #b3b3b3; }

.dash_label-red {
  color: #dd0426; }

.dash_label-blue {
  color: #3f88c5; }

.dash_label-orange {
  color: #f0a202; }

.dash_label-green {
  color: #3d9720; }


.no-profile {
  background-color: #b3b3b3; }

